# Ignore node_modules
node_modules/

# Ignore log files and databases
*.log
*.sql
*.sqlite

# Ignore environment variable files
.env
.env.*

# Allow .env.example
!.env.example

# Ignore Docker files
docker-compose.override.yml
docker-compose.local.yml

# Ignore build directories
build/
dist/

# Ignore system files
.DS_Store
Thumbs.db

# Ignore data folder for MySQL
data/mysql/*
!data/mysql/.gitkeep
data/postgres/*
!data/postgres/.gitkeep

# Ignore editor specific files
.vscode/
.idea/
*.sublime-project
*.sublime-workspace

# Ignore other files generated by IDEs
*.iml
